{
  "extends": "airbnb-base",
  "rules": {
    "no-console": "off",
    "no-restricted-syntax": "off",
    "arrow-parens": "off",
    "comma-dangle": ["error", "never"],
    "max-len": "off",
    "arrow-body-style": "off",
    "no-mixed-operators": "off",
    "no-unused-expressions": "off",
    "import/no-extraneous-dependencies": [
      "error",
      { "devDependencies": ["**/*.test.js", "**/*.spec.js", "**/*.e2e.js"] }
    ]
  },
  "env": {
    "node": true,
    "mocha": true
  }
}
